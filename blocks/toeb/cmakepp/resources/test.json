{
  "name":"json-lang",
  "parsers":{
    "any":"ast_parse_any",
    "sequence":"ast_parse_sequence",
    "empty":"ast_parse_empty",
    "token":"ast_parse_token",
    "list":"ast_parse_list"
  },
  "evaluators":{
    "string":"ast_json_eval_string",
    "number":"ast_json_eval_number",
    "object":"ast_json_eval_object",
    "array":"ast_json_eval_array",
    "boolean":"ast_json_eval_boolean",
    "null":"ast_json_eval_null",
    "key_value":"ast_json_eval_key_value",
    "key_value_list":"ast_json_eval_key_value_list",
    "value_list":"ast_json_eval_value_list"
  },
  "root_definition":"json",
  "definitions":{
    "json":{
      "parser":"any",
      "any":[
        "object",
        "array"
      ]
    },
    "object":{
      "parser":"list",
      "node":"true",
      "begin":"brace_open",
      "element":"key_value",
      "separator":"comma",
      "end":"brace_close"
    },
    "array":{
      "parser":"list",
      "node":"true",
      "begin":"bracket_open",
      "element":"value",
      "separator":"comma",
      "end":"bracket_close"
    },
    "brace_open":{
      "parser":"token",
      "token":"true",
      "match":"{"
    },
    "brace_close":{
      "parser":"token",
      "token":"true",
      "match":"}"
    },
    "bracket_close":{
      "parser":"token",
      "token":"true",
      "match":"]"
    },
    "bracket_open":{
      "parser":"token",
      "token":"true",
      "match":"["
    },
    "nothing":{
      "parser":"empty",
      "empty":"true"
    },
    "key_values":{
      "parser":"any",
      "any":[
        "key_value_list",
        "key_value",
        "nothing"
      ]
    },
    "key_value":{
      "parser":"sequence",
      "node":"true",
      "sequence":[
        "string",
        "colon",
        "value"
      ]
    },
    "key_value_list":{
      "parser":"sequence",
      "node":"true",
      "sequence":[
        "key_value",
        "comma",
        "key_values"
      ]
    },
    "values":{
      "parser":"any",
      "any":[
        "value_list",
        "value",
        "nothing"
      ]
    },
    "value_list":{
      "parser":"sequence",
      "node":"true",
      "sequence":[
        "value",
        "comma",
        "values"
      ]
    },
    "colon":{
      "parser":"token",
      "token":"true",
      "match":":"
    },
    "comma":{
      "parser":"token",
      "token":"true",
      "match":","
    },
    "value":{
      "parser":"any",
      "any":[
        "string",
        "number",
        "object",
        "array",
        "boolean",
        "null"
      ]
    },
    "string":{
      "parser":"token",
      "token":"true",
      "node":"true",
      "replace":"1"   
    },
    "number":{
      "parser":"token",
      "node":"true",
      "token":"true",
      "regex":"([1-9][0-9]*)([.][0-9]+([eE][+-]?[0-9]+)?)?",
      "replace":"1"
    },
    "boolean":{
      "parser":"token",
      "node":"true",
      "token":"true",
      "regex":"true|false"
    },
    "null":{
      "parser":"token",
      "node":"true",
      "token":"true",
      "match":"null"
    },
    "white_space":{
      "parser":"token",
      "token":"true",
      "ignore_token":"true",
      "regex":"[\r\n\t ]+"
    }
  }
}