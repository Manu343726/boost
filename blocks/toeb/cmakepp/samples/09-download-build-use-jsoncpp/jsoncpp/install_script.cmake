function(__ proj package)
  assign(working_dir = package.managed_descriptor.working_dir)
  set(uri "ttps://github.com/open-source-parsers/jsoncpp/archive/0.8.2.tar.gz")
  message(STATUS "downloading jsoncpp - ${uri} - to ${working_dir}")
  assign(success = proj.remote.pull("${uri}" "${working_dir}"))
  message(STATUS "done")
  message(pkd)  
  json_print(${success})

endfunction()